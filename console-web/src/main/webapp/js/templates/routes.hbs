{{#each this}}
    <div class="row">
        <div class="span3">
            <h3>{{routeId}}</h3>
        </div>
        <div class="span6 progress" style="margin-top: 20px;">
            <div class="bar bar-success" style="width: {{percent exchangesCompleted exchangesTotal}}%;"></div>
        </div>
        <div class="span3">
            <h3>{{exchangesCompleted}} / {{exchangesFailed}} / {{exchangesTotal}}</h3>
        </div>
    </div>
{{/each}}
